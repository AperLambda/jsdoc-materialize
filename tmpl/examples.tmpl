<ul class="examples">
  <?js obj.forEach((example) => { ?>
    <li>
      <?js if (example.caption) { ?>
        <p class="code-caption">
          <?js= example.caption ?>
        </p>
      <?js } ?>
      <pre class="prettyprint"><code class="language-js"><?js= this.htmlsafe(example.code) ?></code></pre>
    </li>
  <?js }); ?>
</ul>
